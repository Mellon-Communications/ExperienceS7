        <favorite
            launcher:packageName="com.samsung.android.sm"
            launcher:className="com.samsung.android.sm.ui.dashboard.SmartManagerDashBoardActivity"
            launcher:screen="1"
            launcher:x="1"
            launcher:y="3" />

        <!-- Word -->
        <favorite
            launcher:packageName="com.microsoft.office.word" 
            launcher:className="com.microsoft.office.apphost.LaunchActivity" 
            launcher:screen="1"
            launcher:x="2"
            launcher:y="3" />

